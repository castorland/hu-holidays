{
    "name": "castorland/hu-holidays",
    "description": "Hungarian Holidays Wrapper for the Carbon DateTime Library",
    "keywords": [
        "Hungarian Bank Holiday",
        "Holiday",
        "Holidays",
        "Carbon",
        "Hungarian Holiday",
        "Hungarian Holidays"
    ],
    "type": "library",
    "require": {
        "nesbot/carbon": "^2.67.0",
        "php": "^8.0 || ^8.1 || ^8.2",
        "ext-calendar": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5|^6|^7.5|^8.0|^9.0",
        "php-coveralls/php-coveralls": "^2.67"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Castorland\\HuHolidays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Hódos Gábor",
            "email": "gabor@hodos.me"
        }
    ],
    "config": {
        "discard-changes": true
    },
    "minimum-stability": "dev"
}
